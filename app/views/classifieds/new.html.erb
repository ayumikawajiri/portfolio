<div class="section-body">
	<div class="breadcrumbs" >
    <%= render_breadcrumbs separator: ' > ' %>
  </div>

	<div class="h1-var clearfix">
		<div class="h1-title left">New Classified</div>
		<div class="h1-sub"><%= link_to 'Back', 'javascript:history.back()', class: "right" %></div>
	</div>

	<div class="form">
	  <%= form_with(model: @classified, url: {:action => 'create'}, local: true) do |form| %>
	  	<%= render :partial => 'form', :locals => {:form => form} %>
		<% end %>

		<%#= render 'form', classified: @classified %>
	</div>
</div>